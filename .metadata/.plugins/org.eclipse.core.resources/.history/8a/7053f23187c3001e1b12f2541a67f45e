#ifndef PARALLELRESISTOCONNECTION_H_
#define PARALLELRESISTOCONNECTION_H_

#include "ResistorConnection.h"

class ParallelResistoConnection: public ResistorConnection
{
public:
	ParallelResistoConnection(std::string name);
	virtual ~ParallelResistoConnection();
	char separator() const;
	float nominalValue() const;
	float minimumValue() const;
	float maximumValue() const;
};

#endif /* PARALLELRESISTOCONNECTION_H_ */
