#ifndef RESISTORCONNECTION_H_
#define RESISTORCONNECTION_H_

#include <list>

#include "Resistor.h"

class ResistorConnection: public Resistor
{
protected:
	std::list<std::shared_ptr<Resistor>> resistors;

public:
